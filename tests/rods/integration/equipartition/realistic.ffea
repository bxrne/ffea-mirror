<param>
	<restart = 0>
	<dt = 5e-12>
	<kT = 4.11e-21>
	<check = 5000>
	<num_steps = 2000000> 
	<rng_seed = time>
	<trajectory_out_fname = sphere_63_120_trajectory.ftj>
	<measurement_out_fname = sphere_63_120_measurement.fm>
	<epsilon = 0.01>
	<max_iterations_cg = 1000>
	<kappa = 2e9>
	<epsilon_0 = 1>
	<dielec_ext = 1>
	<calc_stokes = 1>
	<stokes_visc = 1e-03>
	<calc_vdw = 1>
	<calc_noise = 1>
	<calc_kinetics = 0>
	<vdw_type = steric>
	<vdw_cutoff = 5e-10>
   <inc_self_vdw = 0>
	<vdw_steric_factor = 2e3>
	<calc_es = 0>
	<es_update = 4>
	<es_N_x = 50>
	<es_N_y = 30>
	<es_N_z = 30>
	<sticky_wall_xz = 0>
	<wall_x_1 = PBC>
	<wall_x_2 = PBC>
	<wall_y_1 = PBC>
	<wall_y_2 = PBC>
	<wall_z_1 = PBC>
	<wall_z_2 = PBC>
	<es_h = 1>
	<num_blobs = 0>
	<num_rods = 3>
	<num_conformations = (1)>
	<num_states = (1)>
</param>

<system>


<rod>
		<input = bend.rod>
		<output = bend.rodtraj>
		<centroid_pos = (0.0,0.0,0.0)>
		<rotation = (0.00, 0.00, 0.00)>
		<scale = 1>
</rod>
<rod>
		<input = stretch.rod>
		<output = stretch.rodtraj>
		<centroid_pos = (0.0,0.0,0.0)>
		<rotation = (0.00, 0.00, 0.00)>
		<scale = 1>
</rod>
<rod>
		<input = twist.rod>
		<output = twist.rodtraj>
		<centroid_pos = (0.0,0.0,0.0)>
		<rotation = (0.00, 0.00, 0.00)>
		<scale = 1>
</rod>

</system>
